   "source": [
    "x, y = var('x y')\n",
    "\n",
    "u = sin(x+y^3/3)*e^(-y)\n",
    "\n",
    "eqn = (-y^2) * diff(u, x) + diff(u, y)\n",
    "\n",
    "show(eqn)"
